// Test math functions

// Basic math functions
print("sqrt(16): " + sqrt(16));
print("pow(2, 3): " + pow(2, 3));
print("log(100, 10): " + log(100, 10));
print("log10(100): " + log10(100));

// Trigonometric functions
print("sin(0): " + sin(0));
print("cos(0): " + cos(0));
print("tan(0): " + tan(0));

// Inverse trigonometric functions
print("asin(0): " + asin(0));
print("acos(1): " + acos(1));
print("atan(0): " + atan(0));
print("atan2(1, 1): " + atan2(1, 1));

// Angle conversion
print("degrees(3.14159): " + degrees(3.14159));
print("radians(180): " + radians(180));

// Rounding functions
print("floor(3.7): " + floor(3.7));
print("ceil(3.2): " + ceil(3.2));
print("round(3.5): " + round(3.5));
print("round(3.14159, 2): " + round(3.14159, 2));

// Absolute value
print("abs(-5): " + abs(-5));

// Number theory functions
print("gcd(12, 18): " + gcd(12, 18));
print("lcm(12, 18): " + lcm(12, 18));
print("is_prime(7): " + is_prime(7));
print("is_prime(10): " + is_prime(10));
print("factorial(5): " + factorial(5));

// Random number generation
print("random_int(1, 10): " + random_int(1, 10));
print("random_float(): " + random_float());
print("random_uniform(1, 10): " + random_uniform(1, 10));

// Bitwise operations
print("bit_and(5, 3): " + bit_and(5, 3));
print("bit_or(5, 3): " + bit_or(5, 3));
print("bit_xor(5, 3): " + bit_xor(5, 3));
print("bit_not(5): " + bit_not(5));
print("bit_shift_left(5, 1): " + bit_shift_left(5, 1));
print("bit_shift_right(10, 1): " + bit_shift_right(10, 1));
